{
    "name": "webspectyler/laravel-taxonomies",
    "description": "Simple, nestable Terms & Taxonomies (similar to WordPress) for Laravel 6.",
    "homepage" : "https://github.com/webspectyler/Laravel-Taxonomies",
    "license": "MIT",
    "keywords": [
        "taxonomies",
        "terms",
        "categories",
        "nestable",
        "eloquent",
        "model",
        "laravel"
    ],
    "authors": [
        {
            "name"     : "Alexander Manfred Poellmann",
            "homepage" : "http://twitter.com/AMPoellmann",
            "role"     : "developer"
        },
        {
            "name"     : "Tyler Abbott",
            "homepage" : "https://twitter.com/webspecdesign",
            "role"     : "developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support"  : "^8.0",
        "illuminate/database" : "^8.0",
        "cviebrock/eloquent-sluggable" : "^8.0"
    },
    "require-dev": {
        "laravel/framework" : "^8.0",
        "mockery/mockery" : "^1.4.2",
        "phpunit/phpunit" : "^9.3.3",
        "doctrine/dbal": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "Lecturize\\Taxonomies\\" : "src/"
        }
    },
    "extra": {
        "branch-alias" : {
            "dev-master" : "0.0.x-dev"
        }
    },
    "support" : {
        "source" : "https://github.com/Lecturize/Laravel-Taxonomies",
        "issues" : "https://github.com/Lecturize/Laravel-Taxonomies/issues"
    }
}
